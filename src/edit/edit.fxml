<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="335.0" prefWidth="405.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="edit.EditController">
    <GridPane layoutX="103.0" layoutY="59.0" AnchorPane.bottomAnchor="200.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <TextField fx:id="productIdField" GridPane.columnIndex="1"/>
        <TextField fx:id="productNameField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <TextField fx:id="productCountField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <TextField fx:id="productSumField" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <Label text="Артикул"/>
        <Label text="Наименование" GridPane.rowIndex="1"/>
        <Label text="Количество" GridPane.rowIndex="2"/>
        <Label text="Стоимость" GridPane.rowIndex="3"/>
    </GridPane>
    <HBox layoutX="132.0" layoutY="296.0" prefHeight="25.0" prefWidth="141.0" spacing="50.0">
        <Button mnemonicParsing="false" onAction="#handleOk" text="Ок"/>
        <Button mnemonicParsing="false" onAction="#handleCancel" text="Отмена"/>
    </HBox>
</AnchorPane>
